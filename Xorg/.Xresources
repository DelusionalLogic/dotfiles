#include "/home/delusional/.config/xcolors/active"

rofi.color-enabled: true
! State:           'bg',     'fg',     'bgalt',  'hlbg',   'hlfg'
rofi.color-normal: nord0,  nord5,  nord0,  nord13,  nord0
rofi.color-urgent: nord9,  nord0,  nord9,  nord9,  nord0
rofi.color-active: nord14,  nord0,  nord14,  nord11,  nord0

!                  'background', 'border'
rofi.color-window: nord0,      nord13


!Font
urxvt*font: xft:Terminus:pixelsize=12,\
			xft:unifont:pixelsize=12,\
			xft:Inconsolata\ for\ Powerline:pixelsize=12
urxvt*perl-ext-common: default,matcher,selection-to-clipboard
urxvt*url-launcher: xdg-open
urxvt*matcher.button: 1
urxvt*clipboard.autocopy: true
urxvt*iconFile: /usr/share/icons/Moka/48x48/apps/terminal.png
urxvt*depth: 32
! Scollbar
*scrollstyle: plain
urxvt*scrollBar: false
Xft.dpi: 96.0
Xcursor.theme: OpenZone_Ice_Slim
Xcursor.there_core: true
Xcursor.size: 0
URxvt*geometry: 140x40
Xft.rgba: none
Xft.hintstyle: hintfull
